# VALD ForceDecks API Configuration
# Copy this file to .env and fill in your actual VALD API credentials

# VALD API URLs
FORCEDECKS_URL=https://your-vald-instance.forcedecks.com/api
DYNAMO_URL=https://your-vald-instance.forcedecks.com/dynamo
PROFILE_URL=https://your-vald-instance.forcedecks.com/profiles
AUTH_URL=https://your-vald-instance.forcedecks.com/oauth/token

# VALD API Authentication
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here
TENANT_ID=your_tenant_id_here

# Google Cloud Configuration (already have gcp_credentials.json)
# No additional environment variables needed for BigQuery

# Node.js Server Configuration
PORT=4000
NODE_ENV=development